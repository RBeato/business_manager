{
  "name": "business-manager",
  "version": "1.0.0",
  "description": "Centralized business metrics aggregation system",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "setup": "tsx scripts/setup.ts",
    "test:ingestion": "tsx scripts/test-ingestion.ts",
    "ingest": "tsx src/index.ts --ingest",
    "report": "tsx src/index.ts --report",
    "check-credits": "tsx scripts/check-credits.ts",
    "content:generate": "tsx scripts/generate-content.ts",
    "content:generate-biomarker": "tsx scripts/generate-biomarker.ts",
    "content:publish": "tsx scripts/publish-scheduled.ts",
    "content:seed": "tsx scripts/seed-topics.ts",
    "content:seed-biomarkers": "tsx scripts/seed-biomarkers.ts",
    "content:generate-images": "tsx scripts/generate-blog-images.ts",
    "content:backfill-images": "tsx scripts/backfill-blog-images.ts",
    "content:routine-covers": "tsx scripts/generate-routine-covers.ts",
    "setup:ga4": "tsx scripts/setup-ga4.ts",
    "telegram:deploy": "supabase functions deploy telegram-webhook --no-verify-jwt",
    "telegram:set-webhook": "tsx scripts/set-telegram-webhook.ts",
    "revenuecat:deploy": "supabase functions deploy revenuecat-webhook --no-verify-jwt",
    "revenuecat:logs": "supabase functions logs revenuecat-webhook",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.32.0",
    "@google-cloud/billing": "^4.0.0",
    "@octokit/rest": "^20.0.2",
    "@supabase/supabase-js": "^2.45.0",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.5",
    "google-auth-library": "^9.14.0",
    "googleapis": "^144.0.0",
    "imap": "^0.8.19",
    "jsonwebtoken": "^9.0.2",
    "mailparser": "^3.7.1",
    "node-fetch": "^3.3.2",
    "openai": "^6.17.0",
    "resend": "^4.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/imap": "^0.8.40",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/mailparser": "^3.4.4",
    "@types/node": "^22.7.0",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@typescript-eslint/parser": "^8.7.0",
    "eslint": "^9.11.0",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "keywords": [
    "metrics",
    "analytics",
    "business-intelligence",
    "app-store",
    "google-play"
  ],
  "author": "",
  "license": "MIT"
}
